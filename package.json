{
  "name": "sleepycarla",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "preview:network": "vite preview --host",
    "server": "tsx server/server.ts",
    "server:dev": "nodemon --exec tsx server/server.ts",
    "start": "npm run build && npm run server",
    "start:full": "npm run build && concurrently \"npm run server\" \"npm run preview\"",
    "start:network": "npm run build && concurrently \"npm run server\" \"npm run preview:network\"",
    "dev:full": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "railway:build": "npm run build",
    "railway:start": "npm run server"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.1.0",
    "express": "^4.18.2",
    "lucide-vue-next": "^0.525.0",
    "pg": "^8.16.3",
    "sqlite3": "^5.1.7",
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/node": "^24.0.10",
    "@types/pg": "^8.15.4",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "vite": "^7.0.3",
    "vite-plugin-pwa": "^1.0.1",
    "vue-tsc": "^2.2.12",
    "workbox-window": "^7.3.0"
  }
}
